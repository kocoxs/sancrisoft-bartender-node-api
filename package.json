  "description": "the api for the test using nodejs and mysql",
  "main": "index.js",
  "scripts": {
    "dev": "env-cmd -f ./config/dev.env nodemon src/index.js",
    "debug": "env-cmd -f ./config/dev.env node --inspect src/index.js",
