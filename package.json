  "devDependencies": {
    "env-cmd": "^10.1.0",
    "jest": "^25.2.6",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
